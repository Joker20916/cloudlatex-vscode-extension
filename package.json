{
  "name": "cloudlatex",
  "displayName": "cloudlatex",
  "description": "",
  "publisher": "moritanian",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cloudlatex.openWebApp",
    "onView:cloudlatex-commands"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "cloudlatex": [
        {
          "id": "cloudlatex-commands",
          "name": "Commands",
          "when": "config.cloudlatex.enabled"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cloudlatex",
          "title": "cloudlatex",
          "icon": "resources/icon.png"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Cloudlatex",
      "properties": {
        "cloudlatex.enabled": {
          "type": "boolean",
          "default": false,
          "description": "set true if enable cloudlatex plugin"
        },
        "cloudlatex.outDir": {
          "type": "string",
          "default": ".workspace",
          "description": "directory to output compile result"
        },
        "cloudlatex.backend": {
          "type": "string",
          "enum": [
            "cloudlatex"
          ],
          "default": "cloudlatex",
          "description": "backend web application"
        },
        "cloudlatex.email": {
          "type": "string",
          "default": 0,
          "description": "E-mail address"
        },
        "cloudlatex.client": {
          "type": "string",
          "default": "",
          "description": "client id"
        },
        "cloudlatex.token": {
          "type": "string",
          "default": "",
          "description": "access token"
        },
        "cloudlatex.projectId": {
          "type": "number",
          "default": 0,
          "description": "projectId. *Do not mistake this value, otherwise your files might be overwritten."
        }
      }
    },
    "commands": [
      {
        "command": "cloudlatex.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "cloudlatex.refreshEntry",
          "when": "view == cloudlatex-commands",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/pako": "^1.0.1",
    "chokidar": "^3.3.1",
    "eventemitter3": "^4.0.0",
    "form-data": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "node-fetch": "^2.6.0",
    "pako": "^1.0.11"
  }
}
